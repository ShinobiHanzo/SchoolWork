#include <stdio.h>
#define SIZE 5

int main(void) {
	int n[SIZE];
	for (size_t i = 0; i < SIZE; ++i) {
		n[i] = 0;
	}
	printf("%s%13s\n", "Element", "Value");
	for (size_t i = 0; i < 5; ++i) {
		printf("%7u%13d\n", i, n[i]);
	}
}