#include <stdio.h>

int num1, num2, temp;
int;


int main(void) {
    printf("please enter first number: ");
    scanf_s("%d", &num1);
    printf("\nplease enter second number: ");
    scanf_s("%d", &num2);

    printf("After swap: num1 = %d, num2 = %d", num2, num1);
    reference(&num1, &num2);
    
}

void reference(int *num1, int *num2) {
    temp = *num1;
    *num1 = *num2;
    *num2 = temp;
    printf("Call by value: num1 = %d, num2 = %d");
}

void value(int num1, int num2) {
    temp = num1;
    num1 = num2;
    num2 = num1;
    printf("Call by value: num1 = %d, num2 = %d");
}

