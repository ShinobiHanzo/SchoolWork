#include <stdio.h>

int num1, num2, temp;
int;


int main(void) {
    printf("please enter first number: ");
    scanf_s("%d", &num1);
    printf("\nplease enter second number: ");
    scanf_s("%d", &num2);

    swap();
    printf("%d, %d", num1, num2);
}



void swap(num1, num2) {
    temp = num1;
    num1 = num2;
    num2 = num1;
    printf("Wehn values are swapped: %d, %d", num1, num2);
    temp = num1;
    num1 = num2;
    num2 = num1;
    return main();
}
